        });\r\n\r\n      const responseEnv = await res.json();\r\n\r\n      if (responseEnv.error) {\r\n        setMsg({\r\n          formSave: false,\r\n          type: \"error\",\r\n          message: responseEnv.message,\r\n        });\r\n      } else {\r\n        setMsg({\r\n          formSave: false,\r\n          type: \"success\",\r\n          message: responseEnv.message,\r\n        });\r\n      }\r\n    }catch(err){\r\n      setMsg({\r\n        formSave: false,\r\n        type: \"error\",\r\n        message: \"Erro: mensagem nÃ£o cadastrada, tente mais tarde!\",\r\n      });\r\n    }\r\n\r\n    setTimeout(() => {\r\n      setMsg(false);\r\n    }, 2000);\r\n  };\r\n\r\n  const formatDate = (rawDate) => {\r\n    const myDate = new Date(rawDate);\r\n    const d = myDate.getDay();\r\n    const mo = myDate.getMonth() + 1;\r\n    const y = myDate.getFullYear();\r\n    const h = myDate.getHours();\r\n    const mi = myDate.getMinutes();\r\n\r\n    return `${d}/${mo}/${y} - ${h}:${mi}`;\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"header\">\r\n        <div className=\"container\">Contato</div>\r\n      </div>\r\n      <div className=\"section container d-flex justify-content-between\">\r\n        <div className=\"contacts\">\r\n          <div className=\"wtz\">\r\n            <img src={wtz} alt=\"whatsapp\" />\r\n            <p>(11) 98877-4433</p>\r\n          </div>\r\n          <div className=\"email\">\r\n            <img src={email} alt=\"email\" />\r\n            <p>contato@fseltro.com</p>\r\n          </div>\r\n          <form onSubmit={sendForm} name=\"form\">\r\n            <div className=\"form-group\">\r\n              <input\r\n                value={formData.nome}\r\n                onChange={onChangeInput}\r\n                type=\"text\"\r\n                className=\"form-control\"\r\n                id=\"nome\"\r\n                name=\"nome\"\r\n                placeholder=\"Nome:\"\r\n              />\r\n              <input\r\n    